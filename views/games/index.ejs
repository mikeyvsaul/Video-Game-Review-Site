<%- include('../../views/partials/header') %>

<table id="games list">
  <thead>
    <tr>
      <th>Title</th>
      <th>Release<br>Year</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% games.forEach(function(g) { %>
      <tr>
        <td><%= g.name %></td>
        <td><%= g.releaseYear %></td>
        <td><a href="/games/<%= g._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>

<a href="/games/new">Add New Game</a>

<%- include('../../views/partials/footer') %> 

